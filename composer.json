{
    "name": "php-http/psr7-integration-tests",
    "description": "Test suite for PSR7",
    "keywords": [
        "test",
        "psr-7"
    ],
    "homepage": "https://github.com/terrylinooo/psr-http",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com"
        },
        {
            "name": "Terry Lin",
            "email": "contact@terryl.in"
        }
    ],
    "require": {
        "php": "^7.2",
        "phpunit/phpunit": "^8.0",
        "psr/http-message": "^1.0",
        "shieldon/psr-http": "^1.2"
    },
    "require-dev": {},
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Http\\Psr7Test\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Http\\Psr7Test\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
